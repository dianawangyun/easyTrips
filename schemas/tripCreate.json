{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "userId": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "tripName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "startDate": {
      "type": "string",
      "format": "date"
    },
    "endDate": {
      "type": "string",
      "format": "date"
    }
  },
  "additionalProperties": false,
  "required": [
    "userId",
    "tripName",
    "startDate",
    "endDate"
  ]
}
